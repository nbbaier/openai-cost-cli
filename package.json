{
	"name": "openai-cost-cli",
	"version": "1.0.0",
	"description": "CLI tool for tracking OpenAI organization costs and token usage",
	"type": "module",
	"bin": {
		"openai-cost": "src/index.ts"
	},
	"scripts": {
		"start": "bun src/index.ts",
		"cost": "bun src/index.ts",
		"tokens": "bun src/tokenUsage.ts",
		"token-costs": "bun src/tokenCosts.ts",
		"update-prices": "bun src/utils/updatePrices.ts",
		"lint": "bunx biome check .",
		"format": "bunx biome format . --write",
		"lint:fix": "bunx biome check . --write",
		"typecheck": "tsgo --noEmit"
	},
	"keywords": [
		"openai",
		"cost",
		"cli",
		"usage",
		"tokens",
		"monitoring"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.2.3",
		"@types/bun": "^1.2.21",
		"@typescript/native-preview": "^7.0.0-dev.20250909.1"
	},
	"peerDependencies": {
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@better-fetch/fetch": "^1.1.18",
		"temporal-polyfill": "^0.3.0",
		"zod": "^4.1.5"
	},
	"engines": {
		"bun": ">=1.0.0"
	},
	"module": "src/index.ts",
	"private": true
}
